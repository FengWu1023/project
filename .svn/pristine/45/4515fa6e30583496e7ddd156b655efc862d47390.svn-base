.layout {margin:0 auto; width:1200px;}


.topbanner {background: url(../imgs/topbanner.png);height:80px}

.search {height:100px;display: flex;justify-content: space-between;align-items: center;}
/* .search .search-c {} */
.search .search-c input{float:left;margin-bottom: 8px;}
.search .search-c input:first-child {width: 450px;height:30px;border:3px solid #c7181d;padding-left:10px;}
.search .search-c input[type="submit"] {border:0;background:#c7181d url(../imgs/search.png) no-repeat;background-position: center;width:73px; height:36px}
.search .search-c p span {font:12px/2 "";color: #666666;}
.search .search-c p span:nth-child(1) {color: #c7181d;}
.search .search-c p span:nth-child(2) {color: #c7181d;} 

.search a {width:158px;height:34px;border: 1px solid #bdbdbd;font:12px/34px "";display: flex;align-items: center;}
.search a img {width: 16px;height:15px;margin:0 5px 0 10px;}
.search a s {color: #c7181d;font-size:20px;font-weight: 800;}
.search a i {background: url(../imgs/down.png) no-repeat;display: block;height:6px;width:7px;margin-left: 28px;}
.search a span {margin:0 27px 0 10px;}

nav {height:40px;background: #c7181d;}
nav dl {height:40px;font:13px/40px "";color: #ffffff;position: relative;}
nav dl dt {width:158px;background:#b82424;float:left;margin-right: 10px;padding:0 16px;}
nav dl dt b {margin:0 32px 0 10px;}
nav dl dd {float:left;padding:0 26px;}
nav dl dd:hover {background: #6c0709;}
nav ul#category {z-index:3;position: absolute;height:500px;bottom:-500px;background: rgba(255, 255, 255,.8);}
nav ul#category li {height:50px;width:190px;display: flex;color:#000;align-items: center; }
nav ul#category li i {display:block;width:16px;height:16px;background: url(../imgs/nav1.png) no-repeat;margin:0 10px 0 15px;}
.cate-2{width: 979px;height:470px;position:absolute;left:189px;top:40px;background: #ffffff;z-index:10;padding:15px;display: none;}
/* nav #cateshow div:nth-child(4) {display:block;} */
.cate-2-l {float:left}
.cate-2 .cate-2-l div {height:62px;display: flex;align-items: center;width: 660px;margin:10px 0;}
.cate-2 .cate-2-l div h4 {float:left;width:100px;height:50px;font:16px/50px "";color:#000;text-align: center;}
.cate-2 .cate-2-l div ul {float: left;width: 560px;border-bottom:1px solid #ebebeb;padding-bottom: 10px;}
.cate-2 .cate-2-l div ul li {float:left;}
.cate-2 .cate-2-l div ul li a {display: block;padding:0 25px;font:14px/30px "";}
.cate-2 .brands {float:left;width:260px;height:340px;margin:60px 0 0 30px;}



.banner {height:500px;overflow: hidden;width: 100%;position: relative;}
.banner.imgbox{width:100%;height:500px;}
.imgbox img{z-index:1;width:100%;height:500px;position: absolute;left:1424px;top:0}
.imgbox img:nth-child(1){left:0;}


.main {background:#f3f4f6;padding-top:10px;}   
#tab {height:344px;width:1200px;background: #fff;}
#tab ul.tab-t {height:62px;border-bottom: 1px solid #f1f0ec}
#tab ul.tab-t li {float:left;height:60px;font:15px/4 "";width: 210px;}
#tab ul.tab-t li.active {color:red; border-bottom:1px solid red}
#tab ul.tab-t li img {margin:0 4px 0 42px;} 
#tab .tab-b {width:1200px;height:280px;position: relative;overflow: hidden;}
#tab .tab-b .slide {position: absolute;width:1200px;height:720px;top:0}
#tab .tab-b ul {display: flex;justify-content:space-around;}
#tab .tab-b ul li {text-align:center;width: 240px;height:280px;border-right: 1px solid #f1f0ec;display: flex;justify-content:center;flex-direction: column;}
/* #tab .tab-b ul:nth-child(1){background: chartreuse;}
#tab .tab-b ul:nth-child(2){background:#6c0709;}
#tab .tab-b ul:nth-child(3){background:#666666;} */
#tab .tab-b ul li img {width:200px;height:200px;}
#tab .tab-b ul li p {display:block;margin:0 auto;width: 180px;line-height: 20px;;height:20px;white-space:nowrap; text-overflow:ellipsis;overflow:hidden;}
#tab .tab-b ul li span {font:18px/35px "";color:red;}
.ads {height:175px;display: flex;justify-content: space-between;align-items: center;background:#f3f4f6 ;}

.discount {height:400px;background:#f3f4f6;padding-top:15px;overflow: hidden;}
.discount .discount-l{float:left;width: 390px; height:420px;background: #ffffff;}
.discount .discount-l img {margin:10px 0 0 10px;}
.discount .discount-l img:nth-child(3) {margin-left: 30px;}
.discount .d-title {height:60px;display: flex;justify-content: space-between;padding:0 15px;}
.discount .d-title h3 {font:20px/3 "";}
.discount .d-title span {font:15px/4 "";}
.discount .discount-c{float:left;width: 390px; height:420px;background: #ffffff;margin:0 10px;}
.discount .discount-c img {margin:20px 0 10px 10px;}
.discount .discount-c img:nth-child(3) {margin-left: 30px;}

.discount .discount-r{float:left;width: 390px; height:420px;background: #ffffff;}
.discount .discount-r ul li{margin-top:6px;} 
.discount .discount-r ul li a {display: flex;justify-content: space-between;width: 370px;height:105px;border:1px solid #f1f0ec;margin-left:5px;}
.discount .discount-r ul li span {font:28px/2 "";font-weight:800;color:#666;}
.discount .discount-r ul li p {font-size:12px;color:#999;}
.discount .discount-r ul li i {font:12px/2.5 "";color:#333;}
.discount .discount-r ul li s {display: block;width: 65px;height:24px;font:14px/24px "";color:#fff;background: #ff504d;text-align: center;border-radius:10px;margin-top:15px;}

.type-fruit {height:620px;background: #f7f7f7;padding-top:10px;overflow:hidden;}
.type-fruit .type-title {height:50px;display: flex;align-items: center;overflow: hidden;}
.type-fruit .type-title p {width:5px;height:25px;background:#ff7b30;}
.type-fruit .type-title b {font:23px/2 ""; color:#ff7b30;margin:0 8px 0 14px;font-weight: 800;}
.type-fruit .type-title span {font:14px/2 ""; color:#99999b;margin:12px 10px 0 10px;}
.type-fruit .type-title s {margin:12px 0 0 658px;}
.type-fruit .fruit-display {height:560px;border-top:1px solid red;}
.type-fruit .fruit-display img.fruit-ad {float:left;}
.type-fruit .fruit-display .fdisplay-r {float:right;width:920px;height:570px;}
.type-fruit .fruit-display .fdisplay-r ul{display: flex;justify-content:space-between;}
.type-fruit .fruit-display .fdisplay-r ul:first-child {margin-bottom: 10px;}
.type-fruit .fruit-display .fdisplay-r ul li {width:220px;height:270px;background:#fff;text-align:center;display: flex;justify-content:center;flex-direction: column;}
.type-fruit .fruit-display .fdisplay-r ul li img {width:200px;height:200px;}
.type-fruit .fruit-display .fdisplay-r ul li p {display:block;margin:0 auto;width: 180px;line-height: 20px;;height:20px;white-space:nowrap; text-overflow:ellipsis;overflow:hidden;}
.type-fruit .fruit-display .fdisplay-r ul li span {font:18px/35px "";color:red;}


ul.floor {position: fixed;bottom: 150px;left:45px;z-index:20;}
ul.floor li {width:32px;border-bottom: 1px dashed #ccc;}
ul.floor li a {width: 32px;height:32px;background: #f3f4f6;text-align: center;color:#999;font:12px/15px "";border-radius:5px;display: block;margin:10px 0;padding-top:5px;}
ul.floor li:last-child {border: 0;}
ul.floor li img {display:block;margin: 8px auto;}